{
    "Hexo Tag": {
        "type": "emoticon",
        "container": [
            {
                "icon": "{% noteblock guide yellow, 标题 %}\n 正文\n{% endnoteblock %}\n",
                "text": "便条：引导"
            },
            {
                "icon": "{% noteblock idea blue, 标题 %}\n 正文\n{% endnoteblock %}\n",
                "text": "便条：提示"
            },
            {
                "icon": "{% noteblock todo, 标题 %}\n 正文\n{% endnoteblock %}\n",
                "text": "便条：列表"
            },
            {
                "icon": "{% noteblock warning, 标题 %}\n 正文\n{% endnoteblock %}\n",
                "text": "便条：警告"
            },
            {
                "icon": "{% span yellow, \"文字内容\" %}",
                "text": "黄字"
            },
            {
                "icon": "{% del \"文字内容\" %}",
                "text": "删除线"
            },
            {
                "icon": "{% psw \"文字内容\" %}",
                "text": "隐藏文字"
            },
            {
                "icon": "{% kbd \"按键名\" %}",
                "text": "按键图标"
            },
            {
                "icon": "{% gallery stretch midle, 列数, %}\n![图片描述](url)\n{% endgallery %}",
                "text": "图片"
            },
            {
                "icon": "{% folding , \"展开\" %}\n内容\n{% endfolding %}",
                "text": "折叠"
            },
            {
                "icon": "显示文字(url地址 \"链接描述\")",
                "text": "超链接"
            },
            {
                "icon": "{% link 标题::地址url::图标url %}",
                "text": "标签链接"
            },
            {
                "icon": "{% mmedia \"bilibili\" \"bvid:BV号\" %}",
                "text": "B站视频"
            },
            {
                "icon": "{% mmedia \"audio\" \"src:地址\" \"autoplay:true\"%}",
                "text": "音频"
            }
        ]
    }
}
